{
  "name": "prompt-for-user-input-mcp",
  "displayName": "Prompt For User Input MCP",
  "description": "Native input dialogs for MCP user prompts",
  "version": "0.1.0",
  "publisher": "prompt-for-user-input-mcp",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/example/prompt-for-user-input-mcp.git"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "promptForUserInputMcp.showInputDialog",
        "title": "Show Input Dialog"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && npm run copy-files",
    "copy-files": "node -e \"const fs = require('fs'); const path = require('path'); if (!fs.existsSync('out/src')) fs.mkdirSync('out/src', {recursive: true}); fs.copyFileSync('src/webview.html', 'out/src/webview.html'); fs.copyFileSync('src/webview.js', 'out/src/webview.js');\"",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@types/express": "^4.17.17",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
} 